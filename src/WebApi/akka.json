{
    "akka": {
        "actor": {
            "serializers": {
                "hyperion": "Akka.Serialization.HyperionSerializer, Akka.Serialization.Hyperion"
            },
            "serialization-bindings": {
                "System.Object": "hyperion"
            },
            "provider": "cluster",
            "deployment": {
                "/apirouter": {
                    "router": "consistent-hashing-group",
                    "routees": {
                        "paths": ["/user/api"]
                    },
                    "virtual-nodes-factor": 8
                }
            }
        }
    }
}
